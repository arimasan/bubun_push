<p id="notice"><%= notice %></p>

<% @data.each do |attr|%>
  <p><%= attr %></p>
<% end %>  

<h1>Funcs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @funcs.each do |func| %>
      <tr>
        <td><%= link_to 'Show', func %></td>
        <td><%= link_to 'Edit', edit_func_path(func) %></td>
        <td><%= link_to 'Destroy', func, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Func', new_func_path %>
